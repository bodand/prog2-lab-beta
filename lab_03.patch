+# Lab03 project
+# komplex target
+#
+# Originally created: 2023-03-06.
+#
+# CMakeLists.txt --
+#   CMake file a lab_03 projekt komplex céljának fordításához.
+#   Használat:
+#       - parancssor: cmake -B _build -S . && cmake --build _build
+#       - CLion/Visual Studio: Megnyitni a mappát projektként
+#       - Code::Blocks:
+#           1) cmake -G "CodeBlocks - <1>" -B _build -S .
+#               - Windows: <1> = MinGW Makefiles
+#               - Unix: <2> = Unix Makefiles
+#           2) A _build/*.cbp fájlt megnyitni
+
+cmake_minimum_required(VERSION 3.18) # Debian Bullseye
+
+cmake_policy(SET CMP0092 NEW)
+cmake_policy(SET CMP0077 NEW)
+cmake_policy(SET CMP0048 NEW)
+
+include(CheckCXXCompilerFlag)
+
+# Only allow the options to be configured here, if this is the root
+# CMakeLists.txt that is being configured.
+set(LAB3_KOMPLEXMAIN NO)
+if (CMAKE_CURRENT_SOURCE_DIR STREQUAL CMAKE_SOURCE_DIR)
+    set(LAB3_KOMPLEXMAIN YES)
+    option(CPP_WARNINGS_TO_ERRORS "Build with -Werror or equivalent flag. [Default: On]" Yes)
+    option(CPP_ENABLE_MEMTRACE "Enable building with memtrace instrumentation. [Default: On]" Yes)
+endif ()
+
+project(cpp_lab_02_komplex
+        HOMEPAGE_URL "https://git.ik.bme.hu/Prog2/labor_peldak/lab_02"
+        VERSION 1.0.0
+        LANGUAGES C)
+
+add_executable(lab_02_komplex
+               elkeszult.h
+               gtest_lite.h
+               komplex.cpp
+               komplex.h
+               komplex_teszt.cpp)
+
+target_compile_features(lab_02_komplex PRIVATE cxx_std_98)
+set_target_properties(lab_02_komplex PROPERTIES
+                      CXX_EXTENSIONS OFF)
+if (CPP_WARNINGS_TO_ERRORS)
+    target_compile_options(lab_02_komplex PRIVATE
+                           $<$<CXX_COMPILER_ID:GNU,Clang,AppleClang>:-Werror>
+                           $<$<CXX_COMPILER_ID:MSVC>:/WX>)
+endif ()
+
+# Generate warning flags appropriate to the currently used compiler.
+if (LAB3_KOMPLEXMAIN)
+    include("${CMAKE_CURRENT_SOURCE_DIR}/../generate_warnings.cmake")
+endif ()
+
+generate_warnings(CXX lab_02_komplex)
